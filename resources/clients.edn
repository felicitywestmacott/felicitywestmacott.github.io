    :blurb
    "\n\n<p> <i> Official photographs by <a href=\"http://www.imagesstudio.co.uk/\" target=\"_blank\"> Vivien Woods </a> </i></p>"}
   {:image "tyler12.jpg",
    :nextpic "tyler13",
    :prevpic "tyler10",
